{
    "name": "atlas",
    "entities": [
        {
            "name": "template",
            "pattern": "[_/\\\\]+tpl-([a-zA-Z0-9+]+)",
            "directory": "{template}"
        },
        {
            "name": "atlas",
            "pattern": "[/\\\\]+atlas-([a-zA-Z0-9+]+)"
        },
        {
            "name": "hemi",
            "pattern": "[_/\\\\]+hemi-(L|R)"
        },
        {
            "name": "res",
            "pattern": "[_/\\\\]+res-([a-zA-Z0-9]+)"
        },
        {
            "name": "den",
            "pattern": "[_/\\\\]+den-([a-zA-Z0-9]+)"
        },
        {
            "name": "desc",
            "pattern": "[_/\\\\]+desc-([a-zA-Z0-9+]+)"
        },
        {
            "name": "suffix",
            "pattern": "(?:^|[_/\\\\])([a-zA-Z0-9]+)\\.[^/\\\\]+$"
        },
        {
            "name": "extension",
            "pattern": "[^./\\\\](\\.[^/\\\\]+)$"
        }
    ],
    "default_path_patterns": [
        "tpl-{template}/tpl-{template}_atlas-{atlas}[_res-{res}][_desc-{desc}]_{suffix<dseg|probseg|mask>}.{extension<nii|nii.gz|json>|nii.gz}",
        "tpl-{template}/tpl-{template}_atlas-{atlas}[_res-{res}][_den-{den}][_desc-{desc}]_{suffix<dseg|probseg|mask>}.{extension<dlabel.nii|json>|dlabel.nii}",
        "tpl-{template}/tpl-{template}_atlas-{atlas}[_hemi-{hemi}][_den-{den}][_desc-{desc}]_{suffix<dseg|probseg|mask>}.{extension<label.gii|json>|label.gii}",
        "tpl-{template}/tpl-{template}_atlas-{atlas}[_desc-{desc}]_{suffix<dseg|probseg|mask>}.{extension<tsv|json>|tsv}"
    ]
}
